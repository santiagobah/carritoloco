# PostgreSQL Database Configuration
# These are the default values - you can override them by copying this file to .env

# Database host (default: localhost)
DB_HOST=localhost

# Database user (default: your system user)
# On macOS: usually your username
# On Linux: usually 'postgres'
DB_USER=your_username

# Database password (default: empty for local development)
DB_PASS=

# Database name (default: carritoloco)
DB_NAME=carritoloco

# Database port (default: 5432)
DB_PORT=5432
